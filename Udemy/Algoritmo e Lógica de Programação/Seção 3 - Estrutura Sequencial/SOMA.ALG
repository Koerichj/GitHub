Algoritmo "Soma"

Var
 x, y, soma : inteiro

Inicio

      //Entrada de Dados
      Escreva("Digite o valor do X: ")
      Leia(x)
      Escreva("Digite o valor de Y: ")
      Leia(y)
      
      //Precessamento
      soma <- x + y
      
      //Saida de Dados
      Escreval("SOMA = ",soma)

Fimalgoritmo